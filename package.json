{
  "name": "epam_react",
  "version": "1.0.0",
  "description": "React App",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "build:dev": "webpack --env.development --config webpack.server.config.js && webpack --env.development --config webpack.config.js",
    "build:prod": "webpack --env.production --config webpack.server.config.js && webpack --env.production --config webpack.config.js",
    "start": "node ./dist/server.js",
    "flow": "flow"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      "\\.(gif|ttf|eot|svg|jpg)$": "<rootDir>/__mocks__/fileMock.js",
      "setupTestFrameworkScriptFile": "jest-enzyme",
      "testEnvironment": "enzyme"
    },
    "transform": {
      "^.+\\.(js)$": "babel-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js",
      "<rootDir>/node_modules/jest-enzyme/lib/index.js"
    ],
    "collectCoverage": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milena537blr/epam_react.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/milena537blr/epam_react/issues"
  },
  "homepage": "https://github.com/milena537blr/epam_react#readme",
  "dependencies": {
    "classnames": "^2.2.6",
    "cli-react": "^2.1.0",
    "express": "^4.17.0",
    "jest": "^24.8.0",
    "node-sass": "^4.12.0",
    "react-redux": "^7.0.3",
    "react-router": "^5.0.0",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/react": "^8.0.4",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.2",
    "clean-webpack-plugin": "^2.0.2",
    "css-loader": "^1.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-jest": "^22.7.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "file-loader": "^3.0.1",
    "flow-bin": "^0.101.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest-environment-enzyme": "^7.0.2",
    "jest-enzyme": "^7.0.2",
    "mini-css-extract-plugin": "^0.7.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-test-renderer": "^16.8.6",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "ts-loader": "^6.0.2",
    "typescript": "^3.5.1",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "url-loader": "^1.1.2",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.1.1",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-dev-server": "^3.1.8",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-hot-server-middleware": "^0.6.0",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  }
}
